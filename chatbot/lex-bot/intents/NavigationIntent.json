{
    "name": "NavigationIntent",
    "description": "Intent to navigate the website",
    "slots": [
      {
        "name": "Page",
        "slotTypeName": "PageType",
        "valueElicitationSetting": {
          "slotElicitationStyle": "Default",
          "promptSpecification": {
            "messageGroups": [
              {
                "message": {
                  "plainTextMessage": {
                    "value": "Which page would you like to navigate to?"
